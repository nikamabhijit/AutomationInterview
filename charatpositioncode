public class CharacterCount {
	public static void main(String args[]) {
		String str = "abhijit arun nikam";
		int i, len;
		int counter[] = new int[256];

		len = str.length();
  
		for (i = 0; i < len; i++) {
			counter[(int) str.charAt(i)]++;
		}

		for (i = 0; i < 256; i++) {
			
			if (counter[i] != 0) {
				
				System.out.println((char) i + " == " + counter[i]);
			}
		}
	}
}
